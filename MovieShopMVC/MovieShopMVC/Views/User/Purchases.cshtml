<h1>Purchases</h1>
@model List<ApplicationCore.Models.PurchaseRequestModel>
@{
    ViewData["Title"] = "Purchases Page";
}


    <div class="rounded">
        <div class="container-fluid">
            <div class="row">
                @foreach (var movie in Model)
                {
                    <div class="col-6 col-xl-2 col-sm-4 col-lg-3" >
                        <partial name="_MovieCard" model="movie" />
                    </div>

                }

            </div>

        </div>
        
    </div>
    